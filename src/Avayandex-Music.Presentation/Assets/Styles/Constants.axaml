<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="#ffcc00" />
    <SolidColorBrush x:Key="PrimaryForeground" Color="Black" />
    <SolidColorBrush x:Key="NotEnabledForeground" Color="Gray" />
    <SolidColorBrush x:Key="HighlightBrush" Opacity="0.8" Color="#1E33E8" />
    <SolidColorBrush x:Key="LightBackgroundBrush" Color="#EDEDED" />

    <!-- Defaults -->
    <Thickness x:Key="DefBorderThickness">1</Thickness>
    <CornerRadius x:Key="DefCornerRadius">8</CornerRadius>
    <SolidColorBrush x:Key="DefBorderBrush" Color="Gray" Opacity="0.7" />
    <system:Double x:Key="DefLargeFontSize">16</system:Double>
    <system:Double x:Key="DefNormalFontSize">12</system:Double>

    <!-- Controls -->
    <SolidColorBrush x:Key="DefButtonBackground" Color="#D9D9D9" Opacity="0.8" />
    <SolidColorBrush x:Key="PlayerDockPanelBackground" Color="#D9D9D9" />
    <SolidColorBrush x:Key="NavPanelBackground" Color="#CCCCCC" />

</ResourceDictionary>