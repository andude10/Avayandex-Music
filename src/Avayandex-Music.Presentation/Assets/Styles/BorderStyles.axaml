<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Assets/Styles/Constants.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
    <Design.PreviewWith>
        <StackPanel Margin="20">
            <Border Height="200" Width="200"
                    Classes="Nav-panel"></Border>
        </StackPanel>
    </Design.PreviewWith>

    <!-- MainWindow STYLES -->
    <Style Selector="Border.Nav-panel">
        <Setter Property="Background" Value="White"></Setter>
        <Setter Property="BorderBrush" Value="{StaticResource DefBorderBrush}"></Setter>
        <Setter Property="BorderThickness" Value="{StaticResource DefBorderThickness}"></Setter>
    </Style>
</Styles>
